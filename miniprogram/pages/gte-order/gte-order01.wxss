/* pages/gte-order/gte-order01.wxss */
text.title{
  margin-left: 20rpx;
  margin-top:20rpx;
  font-size: 36rpx;
  color:rgba(0, 0, 0, 0.699)
}
view.way{
  color:rgba(170, 175, 174, 0.829);
  margin-left: 20rpx;
  font-size: 28rpx;
}
image.arrow{
  width:30rpx;
  height:30rpx;
}
#pros{
  margin-top:30rpx;
  width:220rpx;
}
#pros>view{
  font-size: 28rpx;
  color:rgba(170, 175, 174, 0.829);
  height:80rpx;
  width:100%;
  text-align: center;
  line-height: 80rpx;
}
view.bag{
  background-color:rgb(255, 255, 255);
  position: fixed;
  bottom: 0rpx;
  width:100%;
  height:90rpx;
  line-height:90rpx;
}
.shopbag{
  width:40rpx;
  height:40rpx;
  position: absolute;
  top:50%;
  left:50rpx;
  transform: translate(0,-50%)
}
.bag>text:nth-child(2){
  font-size:28rpx;
  display: inline-block;
  margin-left: 100rpx;
}
.bag>text.num{
  margin-left:10rpx;
  border-radius: 50%;
  color:#fff;
  width:40rpx;
  height:40rpx;
  background-color:rgb(34, 187, 141);
  display: block;
  position: absolute;
  bottom: 25rpx;
  left:180rpx;
  text-align: center;
  line-height: 40rpx;
  font-size: 23rpx;
}
.bag>text:nth-child(4){
  margin-left: 230rpx;
}
.bag>text:nth-child(5){
  position: absolute;
  right:0;
  background-color:rgb(34, 187, 141);
  display: inline-block;
  height:100%;
  width:200rpx;
  text-align: center;
  color:#fff;
  font-weight: bold;
}
#pros>view.chose{
  border-left: 10rpx solid rgb(34, 187, 141);
  color:rgb(34, 187, 141);
  box-sizing: border-box;
}