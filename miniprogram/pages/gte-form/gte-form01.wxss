/* pages/gte-form/gte-form01.wxss *//* pages/gte-form/gte-form01.wxss */
.list{padding: 20rpx;}
.list+.list{
  border-top: 10rpx solid #f1f1f1;
}
.head{
  justify-content: space-between;
  display: flex;
  padding-bottom: 20rpx;
  font-size: 32rpx;
  border-bottom: 3rpx solid #f1f1f1;
}
.head .title{
  color:rgba(0, 0, 0, 0.4)
}
.head .state{
  color:rgba(0, 0, 0, 0.4)
}
.first{
  justify-content: space-between;
  display: flex;
  padding-top: 20rpx;
  font-size: 32rpx;
}
.second{
  justify-content: space-between;
  display: flex;
  font-size: 26rpx;
  padding-top: 10rpx;
  color: #999;
}
.third{
  justify-content: space-between;
  display: flex;
  padding-top: 20rpx;
  font-size: 30rpx;
  border-top: 3rpx solid #f1f1f1;
  margin-top: 20rpx;
}
.third .total{
  justify-content: space-between;
  display: flex;
}
.third .t-total .t-sum{
  color: rgb(240, 85, 85);
  font-weight: bold;
  font-size: 32rpx;
}
.third .btn{
  justify-content: space-between;
  display: flex;
  font-size: 26rpx;
}
.third .btn .cancel{
  display: block;
  width: 100rpx;
  line-height: 2;
  text-align: center;
  border: 2rpx solid #999;
  margin-right: 30rpx;
}
.third .btn .play{
  display: block;
  width: 160rpx;
  line-height: 2;
  text-align: center;
  border-radius: 10rpx;
  background: #0da862;
  color: #fff;
}
