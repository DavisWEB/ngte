/* pages/gte/login.wxss */
button,.exit{
  background-color:rgb(24, 161, 97);
  color:#fff;
  position:absolute;
  top:50%;
  width:100%;
  transform: translate(0,-50%)
}

.header{
  width:300rpx;
  height:300rpx;
  position: absolute;
  top:10%;
  left:50%;
  transform: translate(-50%,0);
   border-radius: 50%;
   overflow: hidden

}
.header>image{
  width:380rpx;
  height:300rpx;
 
}
.title{
  position:absolute;
  top:40%;
  left:50%;
  transform: translate(-50%,-50%)
}